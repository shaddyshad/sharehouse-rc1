<!--suppress HtmlUnknownTarget -->
<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="initial-scale=1.0"/>
    <title> Sharehouse: Home</title>
    <!-- meta tags -->

    <!-- style files -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.3.3/dist/semantic.min.css">
    <%- include('styles') %>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
</head>
<body id="body">
<div class="ui container fluid">
    <div class="ui segment brand">
        <img class="ui medium image" src="/images/sh_text.png">

    </div>


</div>

<div class="ui container inner">
    <div class="ui two column centered grid">
        <div class="column">
            <form class="ui form" method="post" action="/warehouses">
                <h4 class="ui dividing header">Warehouse detail</h4>
                <div class="field">
                    <input type="text" name="name" placeholder="Name">
                </div>
                <div class="field">
                    <input type="text" name="location" placeholder="Location">
                </div>
                <div class="field">
                    <input type="number" name="area" placeholder="Total Area">
                </div>
                <div class="field">
                    <input type="number" name="empty" placeholder="Empty Area">
                </div>
                <div class="field">
                    <input type="number" name="price" placeholder="Price(per sqm)">
                </div>

                <button class="ui button" type="submit">Submit</button>
            </form>
        </div>
    </div>


    <!-- scripts -->
    <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.3.3/dist/semantic.min.js"></script>
    <script src="/javascripts/script.js" type="text/javascript"></script>

</body>
</html>

